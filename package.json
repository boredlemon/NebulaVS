{
    "name": "NebulaScript",
    "displayName": "NebulaScript",
    "description": "The Nebula Programming Language",
    "version": "2.0.0",
    "icon": "Images/nebula.png", 
    "engines": {
        "vscode": "^1.83.0"
    },
    "publisher": "Toast",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Nebulagigabyte/NebulaVSC"
    },
    "contributes": {
        "languages": [{
            "id": "nebula",
            "aliases": ["nebula", "nebula", "nebula", "nebula"],
            "extensions": [".nb"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "Images/nebula.png",
                "dark": "Images/nebula.png"
            }
        }],
        "grammars": [{
            "language": "nebula",
            "scopeName": "source.nb",
            "path": "./syntaxes/nebula.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "nebula",
                "path": "./snippets/nebula-snippets.json"
            }
        ]
    }
}