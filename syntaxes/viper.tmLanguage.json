{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "viper",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#builtins"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#classes"
		},
		{
			"include": "#namespaces"
		},
		{
			"include": "#methods"
		},
		{
			"include": "#attributes"
		},
		{
			"include": "#types"
		},
		{
			"include": "#statements"
		},
		{
			"include": "#expressions"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#classes"
		},
		{
			"include": "#namespaces"
		},
		{
			"include": "#methods"
		},
		{
			"include": "#attributes"
		},
		{
			"include": "#types"
		},
		{
			"include": "#statements"
		},
		{
			"include": "#expressions"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#constants"
		}
	],
		"repository": {
			"keywords": {
				"patterns": [{
					"name": "keyword.control.vi",
					"match": "\\b(if|while|for|return|then|elseif|else|and|or|not|to|step|end|continue|break)\\b"
				}]
			},
			"strings": {
				"name": "string.quoted.double.vi",
				"begin": "\"",
				"end": "\"",
				"patterns": [
					{
						"name": "constant.character.escape.vi",
						"match": "\\\\."
					}
				]
			},
			"comments": {
				"patterns": [{
					"name":"comment.line.vi",
					"begin": "--",
					"end": "$\\n?"
				}]
			},
			"numbers":{
				"patterns": [{
					"name": "constant.numeric.vi",
					"match": "\\b(0x[0-9a-fA-F]+|[0-9]+(\\.[0-9]*)?|\\.[0-9]+)\\b"
				}]
			},
			"functions": {
				"patterns": [{
					"name": "entity.name.function.vi",
					"begin": "\\bfunction\\s+",
					"end": "\\s*\\-\\>"
				}]
			},
			"variables": {
				"patterns": [{
					"name": "variable.other.vi",
					"match": "\\bimport\\b"
				}]
			},
			"builtins":{
				"patterns": [{
					"name": "support.function.vi",
					"match": "\\b(console|print|while|for|return|then|elseif|else|and|or|not|to|step|end|continue|break)\\b"
				}]
			},
			"constants":{
				"patterns": [{
					"name": "constant.language.vi",
					"match": "\\b(true|false|nil)\\b"
				}]
			}
		},
		"scopeName": "source.vi"
	}
	